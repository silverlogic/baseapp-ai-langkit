[flake8]
ignore = E203, E231, E266, E501, F405, W503
max-complexity = 18
select = B,C,E,F,W,T4,B9
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    *.egg-info,
    dist,
    build,
    .eggs,
    .pytest_cache,
    .mypy_cache,
    .tox,
    testproject/public,
    testproject/static,
